# ============================================
# NATE PROTOCOL - Environment Configuration
# ============================================

# RPC URLs (Get from Alchemy: https://alchemy.com or Infura: https://infura.io)
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_API_KEY
MAINNET_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_API_KEY

# Your wallet private keys (NEVER commit this! NEVER share this!)
# Export from MetaMask: Settings > Security > Export Private Key
SEPOLIA_PRIVATE_KEY=your_testnet_private_key_here
MAINNET_PRIVATE_KEY=your_mainnet_private_key_here

# Etherscan API key (for contract verification)
# Get from: https://etherscan.io/myapikey
ETHERSCAN_API_KEY=your_etherscan_api_key

# Contract addresses (filled after deployment)
CONTRACT_ADDRESS=
COUNCIL_ADDRESS=
TREASURY_ADDRESS=

# Gas reporting
REPORT_GAS=true
